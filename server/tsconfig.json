{
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ES2023",
    "strict": true,
    "esModuleInterop": true,
    "baseUrl": ".",
    "paths": {
      "@/generated/*": ["generated/*"],
      "@/*": ["src/*"]
    }
  },
  "include": ["src/**/*", "generated/**/*"]
}
